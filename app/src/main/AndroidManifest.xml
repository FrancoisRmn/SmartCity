<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henallux.smartcity">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <application
        android:name=".ApplicationObject.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_nconnect"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_nconnect_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.InfoPersos"
            android:label="Informations personelles"
            android:parentActivityName=".view.MainActivity"
            >
        </activity>
        <activity
            android:name=".view.BottomMenu"
            android:label="NConnect"
            >
        </activity>
    </application>

</manifest>